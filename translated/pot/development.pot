msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:77
# b147fc74ff8a435aad19660fa573a9a3
msgid "None or barely noticeable:"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:79
# ebc49c3757dd464a8ca9c5298f28bc2f
msgid "For UTF8 and ASCII datasources: none expected"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:80
# d5e7b2abeacb43008822dceb3e33d06b
msgid "For other datasources: the overhead of the iconv calls should be marginal."
msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:86
#: ../../../en/development/rfc/ms-rfc-104.txt:101
