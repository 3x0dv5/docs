msgid "TYPENAME is replaced by TYPENAMES. FEATUREID is replaced by RESOURCEID."
msgstr ""

#: ../../../en/development/rfc/ms-rfc-105.txt:182
# 5b865d1b574148469c9fa3f1a2146d40
msgid "The <gml:featureMember> element is replaced by <wfs:member>"
msgstr ""

