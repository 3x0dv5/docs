# c613baffe99046d19e3002aa65667667
msgid "Stored queries"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-105.txt:257
# 67154805655f4980946f9d8092af4511
msgid "Stored queries are queries (<wfs:Query>) stored on the server, potentially with parameters whose value can be assigned by the client during a GetFeature request."
msgstr ""

#: ../../../en/development/rfc/ms-rfc-105.txt:260
# c2497f0a1c384fb897647edef6eb7331
msgid "At WEB.METADATA level, a \"wfs_storedqueries\" metadata item can be specified with a comma seperated list of stored queries ids."
msgstr ""

#: ../../../en/development/rfc/ms-rfc-105.txt:263
# 6af0cb7aa53e410c8bec6446335cf674
msgid "Then for each stored query id, \"wfs_{storedqueryid}_inlinedef\" must be specified with a valid <StoredQueryDescription> as value. As it is not always convenient to specify a inline XML content, it is also possible to specify \"wfs_{storedqueryid}_filedeef\" whose value is a filename that contains the <StoredQueryDescription> XML content."
msgstr ""

#: ../../../en/development/rfc/ms-rfc-105.txt:274
# 182b346f486641ec81d71c2b03e9ff31
msgid "with /data/bboxstoredquery.xml containing for example :"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-105.txt:314
# 44f28988eca348bd88ea8d8c769c42f5
msgid "A corresponding valid GetFeature request can be :"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-105.txt:316
# 87bceff1132d497fbc768885740488d9
msgid "REQUEST=GetFeature&STOREDQUERY_ID=bboxstoredquery&LONGMIN=2&LATMIN=49&LONGMAX=3&LATMAX=50"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-105.txt:320
# 63d1fd4e12024693b5266deb2f803bf2
msgid "Predefined urn:ogc:def:query:OGC-WFS::GetFeatureById stored query"
