msgstr ""

#: ../../../en/development/rfc/ms-rfc-94.txt:47
# 888ca48fc3414886be92ec396d71ff57
msgid "smoothing_iteration: The number of iterations of the algorithm. The default is 1."
msgstr ""

#: ../../../en/development/rfc/ms-rfc-94.txt:49
# c8996e6ebeb24ba6bbd340e41e96ba0c
msgid "preprocessing: Optional preprocessing method to add more vertices to the shape prior to smoothing, described below."
msgstr ""

