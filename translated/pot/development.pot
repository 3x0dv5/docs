msgid "MS RFC 57: Labeling enhancements: ability to repeat labels along a line/multiline"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-57.txt:7
#: ../../../en/development/rfc/ms-rfc-60.txt:7
# 2de0ddeeaf5b41bd84a658a72d9f1e99
# b69bdac4ce9541c8813661c60a70bd6c
