# 83bcb5bb73cd4a5da7814a4c4b9ed4b2
msgid "`test mapfile <https://github.com/mapserver/msautotest/blob/branch-6-2/renderers/line_pattern_offset.map>`_ and `resulting image <https://github.com/mapserver/msautotest/blob/branch-6-2/renderers/expected/line_pattern_offset.png>`_"
msgstr ""

#: ../../../en/development/announce/6-2.txt:328
# 14bcf83a108f422a8d2baef77406c341
msgid "**General Speedups and Memory Usage Optimizations**"
msgstr ""

#: ../../../en/development/announce/6-2.txt:330
# 390d43e897f24498ba50185ec41e29ad
msgid "Queries sent to postgis backend have been optimized to exercise eventual indexes that might have been set on the time column."
msgstr ""

#: ../../../en/development/announce/6-2.txt:332
# 0d2e2419e7fa403b8187e8a09d4a6129
msgid "The Postgis driver cuts down on the dynamic memory allocations necessary for simple features."
msgstr ""

#: ../../../en/development/announce/6-2.txt:333
# 3f8294b1f6854fb0bce29a602156bcac
msgid "The labelcache rendering phase has been sped up and consumes less memory."
msgstr ""

#: ../../../en/development/announce/6-2.txt:334
# c557a6639c6e42ce984a60c3e04c7cac
msgid "If using proj version 4.8 or newer, MapServer will no longer use a thread lock before entering proj functions."
msgstr ""

