msgstr ""

#: ../../../en/ogc/wms_dimension.txt:148
# a414c9f3013b44099fa2447dfb43e674
msgid "When MapServer process a valid dimension wms parameter, It will process it into expressions and set it on the :ref:`LAYER` `FILTER` object. If there was already a Logical “MapServer expressions”, It will be concatenated with it."
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:152
# 2df5061bfbe244cca2d2a5d792fbd904
msgid "For example a request such as  &elevation=490/500&... on a MapServer layer (with an empty FILTER) would give .. code-block:: mapfile"
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:155
# beffd893b4104c3ab95f59cb5082be8b
msgid "FILTER (([ELEV] >= 490 AND [ELEV] <= 500))"
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:158
# 2267144296984a8da5c0daba53ad29f9
msgid "For example a request such as  &elevation=600&... on a postgis layer with an existing FILTER  would give .. code-block:: guess"
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:161
# bf08395442014b56968f9e40ab9fbcc0
msgid "FILTER (elev > 500) #before request"
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:162
# c7e7a9f5fd03473cb3c876207e131c5d
