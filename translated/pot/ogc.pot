msgstr ""

#: ../../../en/ogc/wms_dimension.txt:40
# eb6cdc38198749338b4e50ba20bc901d
msgid "To have a valid WMS layer with dimension support, the user has to define the following metadata at the layer level:"
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:43
# 6881497fd5844f909554e10ebb239a73
msgid "*wms_dimensionlist*: (*Mandatory*) comma separated list of dimension names available for the layer"
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:45
# d1161b3cb7214d70bbcf69aab0906165
msgid "*wms_[dimensionname]_item*: (*Mandatory*)  this is the name of the field in the DB that contains the dimension values."
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:48
# 47c850821dae4bde9a4c84f459b5881a
msgid "*wms_[dimensionname]_units*: (*Mandatory*)  Attribute indicating units of dimensional axis. If the dimensional quantity has no units (e.g. band number in a multi-wavelength sensor), use the null string: \"\". If the dimensional quantity has units, unit names should be taken from the Unified Code for Units of Measure (UCUM) if UCUM has an appropriate entry. When UCUM is used, the mandatory units attribute shall be an appropriate entry from the UCUM “name” column."
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:56
# 36a3ad394d6d42b88bd9af216ee83ab5
msgid "*wms_[dimensionname]_extent*: (*Mandatory*)  defines a valid set of values for the dimension"
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:58
# e91fcd592aa3489ba7ec9c26886159fa
msgid "*wms_[dimensionname]_default*: (*Optional*)  this value is used if it is defined and the dimension value is missing in the request."
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:64
# 47d655cea5794a48b5468d18c545107e
msgid "Specifying Dimension Extents"
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:66
# ca255db4a4e446f5856487ca29f56e29
msgid "Dimension Extents can be declared with the following syntax for the *wms_[dimensionname]_extent* metadata (see Annex C.3 in the `WMS 1.1.1 specification`_ document for a full description):"
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:70
# bc39f0d2d8cf4013827f31c5d7d0e382
msgid "*value* - a single value."
msgstr ""

#: ../../../en/ogc/wms_dimension.txt:72
