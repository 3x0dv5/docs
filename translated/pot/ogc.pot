msgstr ""

#: ../../../en/ogc/wms_server.txt:171
#: ../../../en/ogc/wms_server.txt:1241
# 24f1edfd61da4083b19c8570868db0e6
# c065792cce96443fa9e45c1b95a55be1
msgid "wms_onlineresource"
msgstr ""

#: ../../../en/ogc/wms_server.txt:172
# ded79874f4dc4e948b0149bbdfcb2166
msgid "wms_srs (unless PROJECTION object is defined using \"init=epsg:...\")"
msgstr ""

#: ../../../en/ogc/wms_server.txt:173
#: ../../../en/ogc/wms_server.txt:1104
#: ../../../en/ogc/wms_server.txt:1607
# 99bbb3cc1c234e17a04a13e66368f643
# 950e5c59121b4e58a55f0de1b27f96e3
