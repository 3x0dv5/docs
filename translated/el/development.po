msgid "2.3 Performance Implications"
msgstr ""

# 4bed743ff1b34e05a060d0fd807e4e13
#: ../../../en/development/rfc/ms-rfc-103.txt:77
msgid "None or barely noticeable:"
msgstr ""

# 59848b98e55a478f81537c2e94eb2228
