msgstr ""

# 9309024a0e1740729ce92e8a5dca0835
#: ../../../en/development/rfc/ms-rfc-54.txt:331
msgid "Image Creation"
msgstr ""

# c28a9db784c04919a361264613bd45e7
#: ../../../en/development/rfc/ms-rfc-54.txt:333
msgid "nothing special. just pass on to the renderer."
msgstr ""

# aaa00a37275f46e5b166d2572a3a9390
#: ../../../en/development/rfc/ms-rfc-54.txt:336
msgid "Image Saving"
msgstr ""

# ae6c3c9acbea4cd4a84c50c2a69e9c92
#: ../../../en/development/rfc/ms-rfc-54.txt:338
msgid "Depending on whether the renderer can export a raster buffer or not:"
msgstr ""

# ad4b0b043c6143ef8b028b82559eadce
#: ../../../en/development/rfc/ms-rfc-54.txt:340
msgid ""
"supported: get a raster buffer, eventually apply formatoptions "
"(quantization) and pass the buffer to the MapServer's saving functions"
msgstr ""

# c9fd5e829f434c27bbec2494bab5bd70
#: ../../../en/development/rfc/ms-rfc-54.txt:342
msgid "unsupported: call the renderer's saving function"
msgstr ""

# 504a007e62b8436e971b7b69af917844
# 5d5abeb58a91479b9a7167429b4beb59
#: ../../../en/development/rfc/ms-rfc-54.txt:345
#: ../../../en/development/rfc/ms-rfc-58.txt:244
msgid "Raster Layers"
msgstr ""

# 95484ed9d5bb4e42b4073f6a980cb229
#: ../../../en/development/rfc/ms-rfc-54.txt:347
msgid ""
"MapServer's Raster layer handling will be modified so that it can write to a"
" rasterBufferObj and not only to a gdImage."
msgstr ""

# 802b45774a0a4e7eaf16b68da911b485
#: ../../../en/development/rfc/ms-rfc-54.txt:352
