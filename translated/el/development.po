#: ../../../en/development/rfc/ms-rfc-54.txt:229
msgid "Lines"
msgstr ""

# 90f4eee2742a4622bdafc5a63d1ce173
#: ../../../en/development/rfc/ms-rfc-54.txt:231
msgid ""
"simple stroke: is passed a pattern (dashes) and line style (caps, joins)"
msgstr ""

# 3ed23644dd814bc48f9349fc9111d4f5
#: ../../../en/development/rfc/ms-rfc-54.txt:232
