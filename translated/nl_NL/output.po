msgstr ""

# dd3f0d169e9643a1aae040f18c9543ba
#: ../../../en/output/tile_mode.txt:78
msgid ""
"tile=x+y+z tells the server what tile you want to retrieve, using the Google"
" Maps tile addressing system"
msgstr ""

# ceb4eac768eb4cd899bd7adb52d8a889
#: ../../../en/output/tile_mode.txt:80
msgid ""
"*tilemode=ve* tells the server use the Virtual Earth tile naming scheme for "
"the tiles"
msgstr ""

