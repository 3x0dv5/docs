#: ../../../en/MIGRATION_GUIDE.txt:867
msgid "SRS"
msgstr ""

# 53ae340f173842109a9f5cd3911150f6
#: ../../../en/MIGRATION_GUIDE.txt:868
msgid "FORMAT"
msgstr ""

# e7e22ea826ff418dbf6e8d1fb6b63f71
