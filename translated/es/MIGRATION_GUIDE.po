msgid "MapServer Migration Guide"
msgstr ""

# 8d0e8a1bbce646d7a7aae722d15ccb6f
#: ../../../en/MIGRATION_GUIDE.txt:9
msgid "2015-02-12"
msgstr ""

# 3ac19fd4f2b44185bb64c1a96751ca00
