msgid ""
"int type; /\\* By rect, point, shape, attribute, etc... Types match the "
"query functions. \\*/ int qlayer; /\\* used by all functions \\*/"
msgstr ""

# 2ccd710bdd454ef0ae75256874a6234d
#: ../../../en/development/rfc/ms-rfc-52.txt:122
msgid "rectObj *rect; /\\* used by msQueryByRect() \\*/"
msgstr ""

