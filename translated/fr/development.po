msgid "maplayer.c: changes/additions to layer API function"
msgstr ""

# d0d939924ec94010a2479f862c7a9d6d
#: ../../../en/development/rfc/ms-rfc-91.txt:140
msgid ""
"mapshape.c, mappostgis.c, mapogr.c.... : changes/additions to driver "
"functions"
msgstr ""

# 4f3be1deb0db4fa08d8a7ceadad4960b
