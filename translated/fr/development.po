msgstr ""

# b8661c7d9c3a4f2294db42003cb7cdf3
#: ../../../en/development/rfc/ms-rfc-103.txt:50
msgid ""
"The core of MapServer can forget about handling encodings, and will treat "
"all strings as UTF8."
msgstr ""

# e72ce2dfeb534a618426bbcc5a1ebca0
#: ../../../en/development/rfc/ms-rfc-103.txt:52
msgid ""
"All text documents ( e.g. capabilities, getfeature, query, etc.. ) will be "
"emitted as UTF8."
msgstr ""

# e86c9d41c6b747dba92a5d8adc084d07
#: ../../../en/development/rfc/ms-rfc-103.txt:54
msgid ""
"There will be no need to specify encoding metadata entries in the mapfile in"
" order to produce somewhat correct ogc documents."
msgstr ""

# 4ca9c28a2360467180efe60e53113a20
# ea9245de5662434aac65f3d04ed664ff
