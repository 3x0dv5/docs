# e8c4b6bdbcea47949863020a6114016c
#: ../../../en/mapfile/layer.txt:56
msgid "Database connection string to retrieve remote data."
msgstr "Chaîne de caractères pour une connexion distante à une base de données."

# 3f93bae3ec7e4ae290e0a6a17e1b5bbd
#: ../../../en/mapfile/layer.txt:58
msgid ""
"An SDE connection string consists of a hostname, instance name, database "
"name, username and password separated by commas."
msgstr "Une chaîne de caractères pour une connexion SDE est composée d'un nom de serveur (\"hostname\"), d'un nom d'instance, d'un nom de base de données, d'un nom d'utilisateur et de son mot de passe séparés par des virgules."

# e988299050d0429193af69091ac9edeb
#: ../../../en/mapfile/layer.txt:61
msgid ""
"A PostGIS connection string is basically a regular PostgreSQL connection "
"string, it takes the form of \"user=nobody password=****** dbname=dbname "
"host=localhost port=5432\""
msgstr "Une chaîne de caractères pour une connexion PostGIS est sensiblement identique à une connexion pour PostgreSQL, elle prend la forme \"user=nomuser password=****** dbname=dbname host=localhost port=5432\""

# 6febd59a4b9340b98c0d28c51c017e03
#: ../../../en/mapfile/layer.txt:65
msgid "An Oracle connection string: user/pass[@db]"
msgstr "Une chaîne de caractères pour une connexion Oracle s'écrit comme ceci : utilisateur/motdepasse[@basededonnees]."

# b18d9ad056f74705bc9be348b40d6fce
# e051867c54cc4d2798e239bee6fedecc
