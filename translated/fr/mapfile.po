msgid "( ( Expression1 ) AND ( Expression2 ) )"
msgstr "( ( Expression1 ) AND ( Expression2 ) )"

# 168421ea0a064e5598d5a0b0d6744780
#: ../../../en/mapfile/expressions.txt:401
msgid "( ( Expression1 ) && ( Expression2 ) )"
msgstr "( ( Expression1 ) && ( Expression2 ) )"

# ec7428d36cf249f388833e5bfc0f1958
#: ../../../en/mapfile/expressions.txt:403
