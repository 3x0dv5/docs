msgid ""
"returns true when at least one of the logical expressions (Expression1 or "
"Expression2) is true."
msgstr "retourne vrai quand au moins une des expressions logiques (Expression1 ou Expression2) est à vrai."

# deb206e3b2f64840855f163f6687108c
#: ../../../en/mapfile/expressions.txt:420
msgid "NOT ( Expression1 )"
msgstr "NOT ( Expression1 )"

# 6a9b7176e7ca431c8965313f89304ae4
#: ../../../en/mapfile/expressions.txt:422
msgid "! ( Expression1 )"
msgstr "! ( Expression1 )"

# 54c52dd07ec846bfb6614d54a97f7121
#: ../../../en/mapfile/expressions.txt:424
